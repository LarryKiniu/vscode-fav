{
	"name": "vscode-fav",
	"displayName": "vscode-fav",
	"description": "A VS Code extension to favorite most used scripts in package.json and run them by clicking a button",
	"version": "0.0.1",
	"engines": {
		"vscode": "^1.62.0"
	},
	"categories": [
		"Other"
	],
	"activationEvents": [
		"onLanguage:json",
		"onCommand:vscode-fav.helloWorld"
	],
	"main": "./out/extension.js",
	"contributes": {
		"commands": [
			{
				"command": "vscode-fav.fav",
				"title": "Favorite"
			},
			{
				"command": "vscode-fav.unfav",
				"title": "Unfavorite"
			}
		]
	},
	"scripts": {
		"vscode:prepublish": "npm run compile",
		"compile": "tsc -p ./",
		"watch": "tsc -watch -p ./",
		"pretest": "npm run compile && npm run lint",
		"lint": "eslint src --ext ts",
		"test": "node ./out/test/runTest.js"
	},
	"devDependencies": {
		"@types/vscode": "^1.62.0",
		"@types/glob": "^7.1.4",
		"@types/mocha": "^9.0.0",
		"@types/node": "14.x",
		"@typescript-eslint/eslint-plugin": "^5.1.0",
		"@typescript-eslint/parser": "^5.1.0",
		"eslint": "^8.1.0",
		"glob": "^7.1.7",
		"mocha": "^9.1.3",
		"typescript": "^4.4.4",
		"@vscode/test-electron": "^1.6.2"
	},
	"repository": {
		"type": "git",
		"url": "git+https://github.com/LarryKiniu/vscode-fav.git"
	  },
	  "license": "MIT",
	  "bugs": {
		"url": "https://github.com/LarryKiniu/vscode-fav/issues"
	  },
	  "homepage": "https://github.com/LarryKiniu/vscode-fav#readme"
}
